Public Class CalculatorUtilitySharedClass
    Public Shared Function AddValues(var1 As Double, var2 As Double) As Double
        Return var1 + var2
    End Function

    Public Shared Function SubtractValues(var1 As Double, var2 As Double) As Double
        Return var1 - var2
    End Function

    Public Shared Function MultiplyValues(var1 As Double, var2 As Double) As Double
        Return var1 * var2
    End Function

    Public Shared Function DivideValues(var1 As Double, var2 As Double) As Double
        Return var1 / var2
    End Function
End Class
